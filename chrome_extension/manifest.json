{
  "manifest_version": 3,
  "name": "ICESCREAMERSt",
  "version": "1.0",
  "description": "The all-functional abbreviation tool!",
  "permissions": ["scripting", "activeTab"],
  "host_permissions": [
    "http://localhost:5000/*",
    "http://127.0.0.1:5000/*",
    "http://localhost:8000/*",
    "http://127.0.0.1:8000/*",
    "http://127.0.0.1:5001/*",
    "http://127.0.0.1:5001/*"
  ],
  "action": {
      "default_popup": "popup.html",
      "default_icon": {
        "16": "ice_cream.png",
        "48": "ice_cream.png",
        "128": "ice_cream.png"
      }
  },
  "icons": {
      "16": "ice_cream.png",
      "48": "ice_cream.png",
      "128": "ice_cream.png"
  },
   "background": {
    "service_worker": "background.js"
  }
}
